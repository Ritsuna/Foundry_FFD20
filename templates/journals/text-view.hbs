<div class='journal-page-content'>
  {{#if data.title.show}}
  {{#with ffd20}}
  <span class='ffd20 page-nesting'>
    {{#each headers}}
      <span class='parent h{{@key}}'>{{name}}</span>
      {{#if (not @last)}}<span class='page-nesting-separator'>&gt;</span>{{/if}}
    {{/each}}
  </span>
  {{/with}}
  <header class="journal-page-header ffd20">
    <h{{data.title.level}}>{{name}}</h{{data.title.level}}>
  </header>
  {{/if}}
  <section class="journal-page-content ffd20">
    {{{text.enriched}}}
  </section>
</div>
