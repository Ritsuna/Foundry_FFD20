<div class="form-group flexrow descriptors">
  <label>{{localize "FFD20.DescriptorPlural"}}</label>
  <div class="form-fields">
    <ul class="traits-list tag-list">
      {{#each descriptors.selected as |v k|}}
        <li class="tag {{k}}">{{v}}</li>
      {{/each}}
    </ul>
    <a class="trait-selector" data-title="FFD20.DescriptorPlural" data-for="system.descriptors" data-options="spellDescriptors"><i class="fa-solid fa-edit" inert></i></a>
  </div>
</div>
